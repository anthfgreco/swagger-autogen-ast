{
  "openapi": "3.0.0",
  "info": {
    "title": "Test API",
    "version": "1.0.0"
  },
  "servers": [],
  "security": [],
  "paths": {
    "/auth/token": {
      "post": {
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IApiClientTokenResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          }
        },
        "parameters": [],
        "tags": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IApiClientTokenRequest"
              }
            }
          }
        }
      }
    },
    "/patient": {
      "get": {
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IPatient"
                }
              }
            }
          }
        },
        "parameters": [],
        "tags": []
      }
    }
  },
  "components": {
    "schemas": {
      "IApiClientTokenRequest": {
        "type": "object",
        "properties": {
          "clientId": {
            "type": "string"
          },
          "clientSecret": {
            "type": "string"
          }
        },
        "required": [
          "clientId",
          "clientSecret"
        ]
      },
      "IApiClientTokenResponse": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "tokenType": {
            "type": "string"
          },
          "expiresIn": {
            "type": "number"
          }
        },
        "required": [
          "accessToken",
          "tokenType",
          "expiresIn"
        ]
      },
      "IPatient": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "clinic": {
            "type": "string",
            "enum": [
              "CLINICONE",
              "CLINICTWO"
            ]
          }
        },
        "required": [
          "id",
          "firstName",
          "lastName",
          "clinic"
        ]
      }
    },
    "securitySchemes": {}
  }
}